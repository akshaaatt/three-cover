{
  "name": "three-cover",
  "version": "1.2.0",
  "private": false,
  "main": "dist/index.tsx",
  "description": "React component to add 3-D covers",
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist &&  babel components -d dist --copy-files",
    "predeploy": "cd example && yarn install && yarn build",
    "deploy": "gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshaaatt/three-cover.git"
  },
  "keywords": [
    "react",
    "three.js",
    "three",
    "3D",
    "component",
    "cover"
  ],
  "author": "Akshat Tiwari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akshaaatt/three-cover/issues"
  },
  "homepage": "https://akshaaatt.github.io/three-cover/",
  "dependencies": {
    "@react-three/drei": "9.32.3",
    "@react-three/fiber": "8.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "three": "0.139.2",
    "uuid-by-string": "3.0.6",
    "wouter": "2.8.0-alpha.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/three": "^0.144.0",
    "gh-pages": "^4.0.0",
    "typescript": "^4.8.4"
  }
}
